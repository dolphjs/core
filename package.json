{
  "name": "@dolphjs/core",
  "version": "0.0.5beta",
  "description": "dolph is a light-weight, fast, express sugar-coat library for making software development faster ",
  "type": "commonjs",
  "maintainers": [
    "Christopher Egbaaibon <https://github.com/chrisegbaaaibon>",
    "Utibeabasi Ekong <https://github.com/Xlaez>"
  ],
  "contributors": [
    "Andry Rasolofomanana <https://github.com/andryrasolofomanana>",
    "Christopher Egbaaibon <https://github.com/chrisegbaaaibon>"
  ],
  "repository": "https://github.com/dolphjs/core",
  "engines": {
    "node": ">=14.0.0"
  },
  "sponsor": {
    "url": "https://questgig.com"
  },
  "scripts": {
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest --watch",
    "lint: fix": "eslint . --fix",
    "lint": "eslint .",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "dev": "cross-env NODE_ENV=development nodemon tests/manual/server.js",
    "start": "cross-env NODE_ENV=development node tests/manual/server.js",
    "push": "npm publish",
    "build": "tsc",
    "benchmark": "node benchmarks/benchmark/middleware.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/",
    "utils/",
    "config/",
    "index.js",
    "index.d.ts",
    "index.d.ts.map"
  ],
  "keywords": [
    "express",
    "node",
    "dolph",
    "owl",
    "dolphjs",
    "owljs"
  ],
  "author": "Utibeabasi ekong <kamounation@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.13",
    "benchmark": "^2.1.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status": "^1.5.3",
    "moment": "^2.29.4",
    "mongoose": "^6.8.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.2",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.7",
    "cli-table3": "^0.6.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
