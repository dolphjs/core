    "express": "^4.18.2",
    "http-status": "^1.5.3",
    "moment": "^2.29.4",
    "mongoose": "^6.8.2",
    "morgan": "^1.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
